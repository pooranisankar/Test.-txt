#include<iostream>
#include<vector>
using namespace std;
int main() {
	vector<int>nums= {5,7,7,8,8,10};
	int first=-1,last=-1;
	int target=8;
	for(int i=1; i<nums.size(); i++) {
		if(nums[i]==target) {
			if(first==-1)first=i;
			last=i;
		}

	}

	cout<<"first position="<<first;
	cout<<"last position="<<last;
	return 0;
}
